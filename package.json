{
  "name": "brothers",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./server/bin/www",
    "server-watch": "nodemon --watch server --watch \"source-data\" --ignore \"server/public/*\" ./server/bin/www",
    "test": "jest",
    "front-build": "vue-cli-service --mode production build",
    "front-watch": "vue-cli-service --mode dev --watch build",
    "lint": "vue-cli-service lint",
    "lint:ejs": "ejslint",
    "build": "npm run front-build && npm run db:migrate",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "init": "sequelize db:create && sequelize db:migrate"
  },
  "engines": {
    "node": ">=12.16.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "aws-serverless-express": "^3.3.8",
    "bcryptjs": "^2.4.3",
    "camelcase": "^6.0.0",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "connect-flash": "^0.1.1",
    "connect-session-sequelize": "^6.1.1",
    "cookie-parser": "^1.4.5",
    "debug": "~4.1.1",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.1",
    "helmet": "^3.22.0",
    "http-errors": "~1.7.2",
    "logform": "^2.1.2",
    "marked": "^0.7.0",
    "mime": "^2.4.4",
    "moment": "^2.24.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.1",
    "mysql2": "^2.1.0",
    "nconf": "^0.10.0",
    "nodemailer": "^6.4.6",
    "pg": "^8.0.0",
    "pg-hstore": "^2.3.3",
    "read-excel-file": "^4.0.6",
    "sequelize": "^5.21.6",
    "sequelize-cli": "^5.5.1",
    "validator": "^10.11.0",
    "vue": "^2.6.10",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^3.5.3",
    "acorn": "^6.4.1",
    "animate.css": "^3.7.0",
    "axios": "^0.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^4.3.1",
    "bootstrap-notify": "^3.1.3",
    "bootstrap-vue": "^2.10.1",
    "chart.js": "^2.8.0",
    "copy-webpack-plugin": "^5.0.2",
    "dropzone": "^5.7.0",
    "ejs-lint": "^1.1.0",
    "expect": "^24.7.1",
    "font-awesome": "^4.7.0",
    "husky": "^4.2.5",
    "jest": "^24.7.1",
    "jquery": "^3.4.1",
    "lint-staged": "^10.1.7",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "popper.js": "^1.15.0",
    "prettier": "^2.0.5",
    "prettier-config-standard": "^1.0.1",
    "sass-loader": "^7.1.0",
    "select2": "^4.0.6-rc.1",
    "select2-bootstrap4-theme": "^1.0.0",
    "sequelize-erd": "^1.3.0",
    "style-loader": "^0.23.1",
    "supertest": "^4.0.2",
    "vee-validate": "^2.2.2",
    "vue-chartjs": "^3.4.2",
    "vue-image-lightbox": "^6.2.1",
    "vue-lazyload": "^1.2.6",
    "vue-router": "^3.1.6",
    "vue-template-compiler": "^2.6.10",
    "vue-the-mask": "^0.11.1",
    "vuedraggable": "^2.21.0",
    "vuex": "^3.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "yarnhook": "^0.4.3"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-checkout": "yarnhook",
      "post-merge": "yarnhook",
      "post-rewrite": "yarnhook"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint",
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ],
    "*.ejs": [
      "ejslint"
    ]
  }
}
