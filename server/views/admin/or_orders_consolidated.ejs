            <td>
              <%= row.donationsDone %>
            </td>
            <td>
              <%= row.pageNumber %>
            </td>
            <td>
              <%= row.urgencyType %>
            </td>
            <td>
              Chave
            </td>
            <td>
              Quantidade Corrigida
            </td>
            <td>
              Gerência_Distrital_POA
            </td>
            <td>
              Gestão_POA
            </td>
          </tr>
        <% }); %>
      </tbody>
